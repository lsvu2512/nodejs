{{!-- <h1> {{ courses }} </h1> --}}

<div class="mt-4">
    <div class="row">

        {{#each courses}}
        <div class="col-sm-6 col-lg-4">
            <div class="card">
                <img src="..." class="card-img-top" src="{{this.image}}" alt="{{this.image}}">
                <div class="card-body">
                    <h5 class="card-title">{{this.name}}</h5>
                    {{!-- <p class="card-text">{{this.age}}</p> --}}
                    <p class="card-text">{{this.description}}</p>
                    <a href="#" class="btn btn-primary">Love</a>
                </div>
            </div>
        </div>
        {{/each}}

    </div>
</div>